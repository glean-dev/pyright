{
	"compilerOptions": {
		"noUnusedLocals": false,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"strictNullChecks": true,
        "skipLibCheck": true,
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": false,
		"lib" : [ "es2016" ],
		"outDir": "../client/server"
	},
	"exclude": [
		"node_modules"
	]
}
